$(function(){var o=$("#page-preloader"),s=o.find(".spinner");setTimeout(function(){s.fadeOut(),o.delay(800).fadeOut("slow")},2e3),$photo=$(".c-main__photo"),setTimeout(function(){$photo.addClass("js-second")}),setTimeout(function(){$photo.removeClass("js-second")},4500),$photo.on("click",function(){$(this).addClass("js-first"),setTimeout(function(){$photo.removeClass("js-first")},1500)}),$(".c-main__social").mouseover(function(){$(".h-first").removeClass("h-first"),$(".h-last").removeClass("h-last")});$("body"),$("html")});